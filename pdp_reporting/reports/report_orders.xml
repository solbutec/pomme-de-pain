<odoo>
    <report
            id="report_pos_order"
            model="pos.order"
            menu="False"
            string="Report"
            name="pdp_reporting.report_pos_order_view"
            file="Current hand"
            report_type="qweb-pdf"/>

    <template id="report_pos_order_view">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="docr">
                <div class="page article">
                    <div class="header">
                        <div class="row">
                            <div class="col-4"/>
                            <div class="col-4 text-center">
                                MAIN
                            </div>
                            <div class="col-4" />
                        </div>
                    </div>
                </div>
            </t>
        </t>
    </template>
</odoo>