<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <report
            id="report_main_courante"
            model="pos.config"
            string="Main courante"
            name="pdp_reporting.integration_report_view"
            file="pdp_reporting.integration_report_view"
            menu="False"
            report_type="qweb-pdf"/>

    <template id="integration_report_view">

        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-set="doc" t-value="doc.sudo()"/>
                <div class="header">
                <img t-if="doc.company_id.logo" t-att-src="image_data_uri(doc.company_id.logo)" class="float-left"
                         alt="Logo"/>
                </div>
                <div class="page article">
                    Article
                </div>
            </t>
        </t>
    </template>
</odoo>